@model HomeVM
@{
    ViewData["Title"] = "Home Page";
}


      <div class="slider-area">
        <!-- Main Slider -->
        <div class="swiper-container main-slider swiper-arrow with-bg_white">
          <div class="swiper-wrapper">
              @foreach (var item in Model.Sliders)
            {
                <div class="swiper-slide animation-style-01">
                  <div
                    class="slide-inner style-1 bg-height"
                    data-bg-image="/images/slider/bg/1-1.jpg"
                  >
                    <div class="container">
                      <div class="row">
                        <div class="col-lg-6 order-2 order-lg-1 align-self-center">
                          <div class="slide-content text-black">
                            <span class="offer">@item.Precent</span>
                            <h2 class="title">@item.Label</h2>
                            <p class="short-desc">
                              @item.Description
                            </p>
                            <div class="btn-wrap">
                              <a
                                class="btn btn-custom-size xl-size btn-pronia-primary"
                                asp-controller="Shop" asp-action="Index"
                                >Discover Now</a
                              >
                            </div>
                          </div>
                        </div>
                        <div
                          class="col-lg-6 col-md-8 offset-md-2 offset-lg-0 order-1 order-lg-2"
                        >
                          <div class="inner-img">
                            <div class="scene fill">
                              <div class="expand-width" data-depth="0.2">
                                <img
                                  src="~/images/dbphoto/@item.Image"
                                  alt="Inner Image"
                                />
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
            }
          </div>
          <!-- Add Pagination -->
          <div class="swiper-pagination d-md-none"></div>

          <!-- Add Arrows -->
          <div class="swiper-button-prev"></div>
          <div class="swiper-button-next"></div>
        </div>
      </div>
      <!-- Slider Area End Here -->



      <!-- Begin Shipping Area -->
      <div class="shipping-area section-space-top-100">
         <div class="container">
            <div class="shipping-bg">
                <div class="row shipping-wrap">
                   @foreach (var item in Model.Shippings)
                   {
                      <div class="col-lg-4 col-md-6 mt-4 mt-md-0">
                         <div class="shipping-item">
                               <div class="shipping-img">
                                 <img
                                   src="~/images/dbphoto/@item.Image"
                                   alt="Shipping Icon"
                                 />
                               </div>
                               <div class="shipping-content">
                                 <h2 class="title">@item.Label</h2>
                                 <p class="short-desc mb-0">@item.Description</p>
                               </div>
                         </div>
                      </div>
                   }
                </div>
            </div>
         </div>
      </div>
      <!-- Shipping Area End Here -->



