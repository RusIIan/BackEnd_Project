@model Slider

<div class="container">
    <h1>Slider Details</h1>
    <div class="row">
        <div>
            <img src="~/images//dbphoto/@Model.Image" width="200px" height="200px" />
            <h1>@Model.Label</h1>
            <h2>@Model.Percent</h2>
            <h3>@Model.Description</h3>
        </div>
    </div>
</div>